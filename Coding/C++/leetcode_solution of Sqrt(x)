class Solution {
public:
    int mySqrt(int x) {

        if(x == 0){
            return 0;
        }

        long long s = 1;
        long long e = x;

        while(s<=e){

            long long mid = s+(e-s)/2;
            long long sq = mid*mid;

            if(sq == x){
                return mid;
            }
            else if(sq < x){
                s = mid + 1;
            }
            else{
                e = mid - 1; 
            }
        }
        return e;
    }
};

ListNode* reverse(ListNode* &head, ListNode* &newhead){
    if(head->next==NULL){
        newhead=head;
        return head;
        }
        ListNode* temp=reverse(head->next,newhead);
        temp->next=head;
        return head;
    }
    ListNode* reverseList(ListNode* head) 
    {    
           if(head==NULL || head->next==NULL){
            return head;
        }
        ListNode* newhead=NULL;
       ListNode* temp=reverse(head,newhead);
       // head->next=NULL;
       temp->next=NULL;
 
        return newhead;
    }
